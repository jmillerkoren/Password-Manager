{
  "name": "FreePass",
  "version": "1.0",
  "description": "Free password manager",
  "manifest_version": 2,
  "permissions":[
    "cookies",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
